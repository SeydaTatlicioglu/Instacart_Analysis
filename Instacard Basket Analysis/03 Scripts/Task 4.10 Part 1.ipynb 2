   "source": [
    "# Creating a 'region' column\n",
    "result=[]\n",
    "for state in df['state']:\n",
    "    if state in Northeast:\n",
    "        result.append('Northeast')\n",
    "    elif state in Midwest:\n",
    "        result.append('Midwest')\n",
    "    elif state in South:\n",
    "        result.append('South')\n",
    "    else:\n",
    "        result.append('West')"
   ]
  },
